<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="info.palamarchuk.api.cooking.entity.Tag" table="tag" schema="palinfo_cooking">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(40)" length="40"/>
        </property>
    </class>
</hibernate-mapping>