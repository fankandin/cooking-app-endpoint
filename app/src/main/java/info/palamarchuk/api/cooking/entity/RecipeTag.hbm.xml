<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="info.palamarchuk.api.cooking.entity.RecipeTag" table="recipe_tag" schema="palinfo_cooking">
        <composite-id mapped="true" class="info.palamarchuk.api.cooking.entity.RecipeTagPK">
            <key-property name="recipeId">
                <column name="recipe_id" sql-type="int(10) unsigned"/>
            </key-property>
            <key-property name="tagId">
                <column name="tag_id" sql-type="int(10) unsigned"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>