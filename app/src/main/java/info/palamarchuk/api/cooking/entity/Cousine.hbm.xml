<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="info.palamarchuk.api.cooking.entity.Cousine" table="cousine" schema="palinfo_cooking">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="nameRu">
            <column name="name_ru" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="nameEn">
            <column name="name_en" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="nameDe">
            <column name="name_de" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>