<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="info.palamarchuk.api.cooking.entity.Reference" table="reference" schema="palinfo_cooking">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(80)" length="80"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>